{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "types": ["@vitest/browser/providers/playwright","jest", "@testing-library/jest-dom"],
  },
  "include": ["vite.config.ts"],
  "exclude": ["tests/**/*"]
}
